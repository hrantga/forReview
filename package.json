{
  "name": "calendar",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "classnames": "^2.2.5",
    "lodash": "^4.17.5",
    "moment": "^2.21.0",
    "moment-timezone": "^0.5.14",
    "node-sass-chokidar": "^1.1.0",
    "npm-run-all": "^4.1.2",
    "rc-calendar": "^9.6.0",
    "rc-time-picker": "^3.3.1",
    "react": "^16.3.2",
    "react-country-region-selector": "^1.2.3",
    "react-dom": "^16.3.2",
    "react-moment": "^0.7.0",
    "react-moment-input": "^0.2.13",
    "react-redux": "^5.0.7",
    "react-scripts": "1.1.1",
    "redux": "^3.7.2",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0",
    "semantic-ui-react": "^0.78.3"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
